@model UploadExcelFilesModel

@{
    ViewData["Title"] = "Загруженные файлы";
}
@if (Model.LoadSuccessful)
{
    <h1>Успешно!</h1>
    <ul>
        @foreach (var file in Model.Files)
        {
            <li>@file.Type - @file.Name</li>
        }
    </ul>
}

@if (!Model.LoadSuccessful)
{
    <h1>Незагружено!</h1>
}